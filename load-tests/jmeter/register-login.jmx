            <stringProp name="script">int n = ctx.getThreadNum();
String username = "user" + n;
String email = "user" + n + "@example.com";
String password = "Password123!";
vars.put("username", username);
vars.put("email", email);
vars.put("password", password);
</stringProp>
          </JSR223PreProcessor>
          <hashTree/>
